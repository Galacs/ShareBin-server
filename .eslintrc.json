{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["jest"],
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "import/extensions": 0,
        "consistent-return": 0,
        "jest/expect-expect": 0,
        "jest/no-disabled-tests": 0
    }
}
